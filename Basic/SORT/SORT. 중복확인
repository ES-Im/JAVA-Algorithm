import java.util.*;

public class Main {
    // 1. stack.contain 이용
    static Character solution(int[] arr) {
        Stack<Integer> stack = new Stack<>();
        for (int i : arr) {
            if(!stack.contains(i)) stack.add(i);
            else return 'D';
        }
        return 'U';
    }

    // 2. Arrays.sort 이용
    static Character solution1(int len, int[] arr) {
        Arrays.sort(arr);
        for (int i = 0; i < len - 1; i++) {
            if(arr[i] == arr[i+1]) return 'D';
        }

        return 'U';
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int len = sc.nextInt();
        int[] arr = new int[len];
        for (int i = 0; i < len; i++) {
            arr[i] = sc.nextInt();
        }

        System.out.println(solution(len, arr));
    }
}
