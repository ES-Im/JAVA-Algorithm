/*  결정알고리즘 연습(예산 배정)
    결정액
    총 예산 : 485
    요청 예산 : 120, 110, 140, 150
    총 예산 내에서 각 지방자치단체에 할당 할 수 있는 상한액
    input '
    4 485
    120 110 140 150
    '
 */

import java.util.*;

public class Main {

    static int calculate(int[] arr, int cap) {
        int sum = 0;
        for (int x : arr) {
            sum += Math.min(x, cap);
        }
        return sum;
    }

    static int solution(int[] arr, int budget) {
        int lt = 0;
        int rt = Arrays.stream(arr).max().getAsInt(); // 요청한 금액 중 최댓값
        int result = 0;

        while (lt <= rt) {
            int mid = (lt + rt) / 2;
            if (calculate(arr, mid) <= budget) {
                result = mid; // 조건 만족 → 더 큰 cap 가능
                lt = mid + 1;
            } else {
                rt = mid - 1; // 조건 초과 → cap 줄여야 함
            }
        }

        return result;
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt(); // 지방 수
        int[] arr = new int[n];
        for (int i = 0; i < n; i++) {
            arr[i] = sc.nextInt();
        }
        int budget = sc.nextInt(); // 총 예산

        System.out.println(solution(arr, budget));
    }
}
